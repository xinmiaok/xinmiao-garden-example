@thesis{HuangKaoLuJianZhuHuoZaiYinSuYingXiangDeRenQunShuSanZhiNengTiFangZhenPingTaiYanJiu2018,
  type = {硕士},
  title = {考虑建筑火灾因素影响的人群疏散智能体仿真平台研究},
  author = {黄, 璜},
  date = {2018},
  institution = {{清华大学}},
  doi = {10.27266/d.cnki.gqhau.2018.000522},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C475KOm_zrgu4lQARvep2SAkEcTGK3Qt5VuzQzk0e7M1z1ZEv7Jrd6asHVOcLGwtWcfGbTND0MsCUwGrohz8i_R0&uniplatform=NZKPT},
  urldate = {2023-06-08},
  abstract = {研究建筑物火灾情境下的人群疏散,对于保证生命安全、减少人员伤亡具有重要的意义。随着计算机技术的发展,对火灾情境下人员疏散进行仿真模拟现已成为目前疏散研究的主要方向。目前多数疏散仿真模型忽视了火灾及其产物对疏散者造成的影响,导致仿真结果与实际产生偏差。少数考虑火灾影响的仿真模型,通常简化了对疏散者行为的模拟。基于多智能体(Multiagent system,MAS)的模型是当前主流的一类疏散模型,它通过对个体微观行为的建模来反映系统宏观的涌现性特征,能够较好地对疏散者行为进行模拟。因此本研究的目的是采用多智能体建模思想建立考虑建筑火灾因素实时影响的疏散仿真模型,以实现对建筑火灾情境下人群疏散更加精确的仿真。本研究通过文献调研的方式全面梳理了火灾及其产物对疏散者生理和行为决策的影响机制,并将其转化为适用于多智能体建模的行为规则。在生理方面主要考虑了火灾及其产物对疏散者的可视范围、移动速度以及健康程度的影响,在行为决策方面主要考虑了疏散者躲避危险的机制。在此基础上,本研究对基于多智能体的疏散软件SAFEgress进行二次开发,实现其与火灾模拟软件Pyrosim的交互,建立了考虑火灾实时影响的人群疏散仿真平台FREEgress(Fire Risk Emulated Environment for Egress)。本研究通过将FREEgress与SAFEgress以及目前典型商业火灾疏散仿真软件FDS+Evac的仿真结果进行对比,验证了FREEgress仿真平台的有效性。在此基础上运用FREEgress开展案例研究,通过大规模的疏散仿真实验,研究了不同火源位置、延迟时间和疏散者行为模式对疏散过程和疏散结果的影响。仿真结果表明上述三个因素各自对疏散均会造成影响,而另外两种因素的存在则一定程度上会改变单因素对于疏散的影响。本研究基于仿真结果对火灾应急管理提出了针对性建议。本研究系统梳理了火灾对疏散者疏散决策和行为的影响机制,并且建立了考虑火灾实时影响的疏散仿真平台,实现了对建筑物火灾情境下人群疏散更精确的模拟。研究成果在研究层面有助于分析和理解火灾情境下人群的疏散规律,在应用层面有助于消防管理部门预测和管理火灾的应急疏散过程,对于建筑物疏散性能评估、优化建筑物性能化防火设计亦具有重要意义。},
  langid = {chinese},
  keywords = {crowd evacuation,FREEgress,FREEgress building fire,multi-agent simulation system,人群疏散,多智能体仿真(MAS),建筑火灾},
  file = {..\\文件,阅读\\zotero,存储\\storage\\3PA3YQS7\\黄 - 2018 - 考虑建筑火灾因素影响的人群疏散智能体仿真平台研究.pdf}
}

@article{huangTrackingJobHousing2018,
  title = {Tracking Job and Housing Dynamics with Smartcard Data},
  author = {Huang, Jie and Levinson, David and Wang, Jiaoe and Zhou, Jiangping and Wang, Zi-jia},
  date = {2018-12-11},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {Proc. Natl. Acad. Sci. U.S.A.},
  volume = {115},
  number = {50},
  pages = {12710--12715},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1815928115},
  url = {https://pnas.org/doi/full/10.1073/pnas.1815928115},
  urldate = {2023-07-17},
  abstract = {Residential locations, the jobs–housing relationship, and commuting patterns are key elements to understand urban spatial structure and how city dwellers live. Their successive interaction is important for various fields including urban planning, transport, intraurban migration studies, and social science. However, understanding of the long-term trajectories of workplace and home location, and the resulting commuting patterns, is still limited due to lack of year-to-year data tracking individual behavior. With a 7-y transit smartcard dataset, this paper traces individual trajectories of residences and workplaces. Based on in-metro travel times before and after job and/or home moves, we find that 45 min is an inflection point where the behavioral preference changes. Commuters whose travel time exceeds the point prefer to shorten commutes via moves, while others with shorter commutes tend to increase travel time for better jobs and/or residences. Moreover, we capture four mobility groups: home mover, job hopper, job-and-residence switcher, and stayer. This paper studies how these groups trade off travel time and housing expenditure with their job and housing patterns. Stayers with high job and housing stability tend to be home (apartment unit) owners subject to middle- to high-income groups. Home movers work at places similar to stayers, while they may upgrade from tenancy to ownership. Switchers increase commute time as well as housing expenditure via job and home moves, as they pay for better residences and work farther from home. Job hoppers mainly reside in the suburbs, suffer from long commutes, change jobs frequently, and are likely to be low-income migrants.},
  langid = {english},
  file = {..\\文件,阅读\\zotero,存储\\storage\\KHQFLAYX\\Huang 等 - 2018 - Tracking job and housing dynamics with smartcard d.pdf}
}

