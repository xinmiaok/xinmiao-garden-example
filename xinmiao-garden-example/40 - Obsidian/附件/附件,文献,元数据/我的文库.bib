@thesis{ZhangFuZaXiTongDeYiZhiXingYanJiu2011,
  type = {博士},
  title = {复杂系统的一致性研究},
  author = {张, 青},
  date = {2011},
  institution = {{武汉大学}},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C447WN1SO36whHG-SvTYjkCc7dJWN_daf9c2-IbmsiYfKqKtIZLem5CJP2shxSembTojJ3AeMndMRaT7oO1Zi2GM&uniplatform=NZKPT},
  urldate = {2023-06-08},
  abstract = {近年来,多主体系统的建模与控制已经成为复杂系统研究的一个焦点。一方面,随着网络通信技术,机器人技术和微型传感器技术的发展,工程领域中出现了大量的分布式系统的控制问题,如集群现象、编队控制、分布式计算、协调控制等。在这样的背景下,原有的针对单一决策主体和单一决策目标的传统控制理论已不能解决网络化系统中产生的新问题。另一方面,在生物以及社会、经济系统中存在大量的自组织现象,即不同个体间局部的相互作用可以产生稳定的群体行为,这些亟需严谨的理论分析。以上两者共同促进了多主体系统的发展,作为复杂系统的一种,多主体系统已经受到学者们的广泛关注,已有的研究成果涉及数学、工程技术、社会科学、物理学以及医学、生物学等众多不同的学科。一致性问题是多主体系统研究中的一个基本问题,本文重点考虑了带有领导者的二阶控制多主体系统的一致性问题,结合控制理论中常用的一些方法设计一致性协议,同时还研究了混沌系统混合同步的问题。文章的主要内容概括如下：1.自适应控制下一致性问题的研究在这个问题中,所研究的多主体系统中各主体之间的连接拓扑为均衡的有向图,本文提出了基于自适应控制的一致性协议,考虑了系统中节点在信息传输时产生时滞时的现象,利用Lyapunov—Krosovskii稳定性理论,得到了使得带领导者多主体系统实现一致性的充分条件,并且估计了时滞上界。2.脉冲控制下一致性问题的研究本文设计了基于脉冲控制的二阶一致性算法,所研究的多主体系统具有向拓扑连接,并且考虑网络拓扑是固定或切换两种情况,领导者的速度是时变的。运用Lyapunov稳定性理论证明了在脉冲增益矩阵以及脉冲间隔满足一定条件时,带领导者的多主体系统能达到一致。3.随机扰动下多主体系统的一致性噪声的干扰在信号的传输过程中是无法避免的,同时噪声也是对系统稳定性的干扰。针对这一现象,文章提出了带噪声干扰的二阶一致性算法。研究主体间的连接拓扑为有向图,并且信号传输具有时滞的带领导者的多主体系统的一致性。运用随机微分时滞系统的分析方法,给出了均方意义下带领导者多主体系统达到一致的充分条件。4.混沌系统的混合同步在混沌系统的同步研究中,一般研究的都是各状态变量实现一种同步现象的情形,而本文讨论了混沌系统的混合同步现象,具体研究了广义Lorenz系统中一部分状态变量实现完全同步、其余状态量实现反同步的混合同步现象。分别运用线性反馈和自适应反馈的方法设计控制器。运用Lyapunov稳定性理论证明了控制的有效性。最后,在总结全文的基础上,对将来准备进行的研究工作进行展望。},
  langid = {chinese},
  keywords = {adaptive control,anti-synchronization,impulsive control,stochastic disturbance,switching topologies,synchroniza-tion,time-delay,切换拓扑,反同步,多主体系统,完全同步 Multi-agent systems,时滞,脉冲控制,自适应控制,随机扰动},
  file = {..\\文件,阅读\\zotero,存储\\storage\\M9QMWN7Z\\张 - 2011 - 复杂系统的一致性研究.pdf}
}

