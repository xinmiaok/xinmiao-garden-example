}

@article{barzelConstructingMinimalModels2015,
  title = {Constructing Minimal Models for Complex System Dynamics},
  author = {Barzel, Baruch and Liu, Yang-Yu and Barabási, Albert-László},
  date = {2015-05-20},
  journaltitle = {Nature Communications},
  shortjournal = {Nat Commun},
  volume = {6},
  number = {1},
  pages = {7186},
  publisher = {{Nature Publishing Group}},
  issn = {2041-1723},
  doi = {10.1038/ncomms8186},
  url = {https://www.nature.com/articles/ncomms8186},
  urldate = {2023-09-20},
  abstract = {One of the strengths of statistical physics is the ability to reduce macroscopic observations into microscopic models, offering a mechanistic description of a system’s dynamics. This paradigm, rooted in Boltzmann’s gas theory, has found applications from magnetic phenomena to subcellular processes and epidemic spreading. Yet, each of these advances were the result of decades of meticulous model building and validation, which are impossible to replicate in most complex biological, social or technological systems that lack accurate microscopic models. Here we develop a method to infer the microscopic dynamics of a complex system from observations of its response to external perturbations, allowing us to construct the most general class of nonlinear pairwise dynamics that are guaranteed to recover the observed behaviour. The result, which we test against both numerical and empirical data, is an effective dynamic model that can predict the system’s behaviour and provide crucial insights into its inner workings.},
  issue = {1},
  langid = {english},
  keywords = {Statistical physics,Theoretical physics},
  annotation = {abstractTranslation: abstractTranslation: 统计物理学的优势之一是能够将宏观观测简化为微观模型，提供系统动力学的机械描述。这种范式植根于玻尔兹曼的气体理论，已经找到了从磁性现象到亚细胞过程和流行病传播的应用。然而，这些进步中的每一个都是数十年精心建立和验证模型的结果，这些模型不可能在缺乏准确微观模型的大多数复杂的生物、社会或技术系统中复制。在这里，我们开发了一种方法，通过观察复杂系统对外部扰动的响应来推断其微观动力学，使我们能够构建最一般的非线性成对动力学，保证恢复观察到的行为。我们根据数值和经验数据测试的结果是一个有效的动态模型，可以预测系统的行为并为其内部工作提供重要的见解。},
  file = {..\\文件,阅读\\zotero,存储\\storage\\55FVU535\\Barzel 等 - 2015 - Constructing minimal models for complex system dyn.pdf}
}

@article{barzelUniversalityNetworkDynamics2013,
