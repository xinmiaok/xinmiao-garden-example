@article{chengNetworkbasedApproachPrediction2018,
  title = {Network-Based Approach to Prediction and Population-Based Validation of in Silico Drug Repurposing},
  author = {Cheng, Feixiong and Desai, Rishi J. and Handy, Diane E. and Wang, Ruisheng and Schneeweiss, Sebastian and Barabási, Albert-László and Loscalzo, Joseph},
