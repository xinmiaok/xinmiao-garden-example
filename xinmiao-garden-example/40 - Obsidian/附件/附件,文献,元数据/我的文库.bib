@thesis{LiuChengShiGuiDaoJiaoTongShuNiuDiQuDeFenXingWangLuoLianJieYanJiu2011,
  type = {硕士},
  title = {城市轨道交通枢纽地区的分形网络连接研究},
  author = {刘, 沙沙},
  date = {2011},
  institution = {{大连理工大学}},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C475KOm_zrgu4lQARvep2SAkhskYGsHyiXksOA0FurgRFZbIi3Ai1KII9BSqHtYnH8fnW0i4OPFfqOovSstoWOQt&uniplatform=NZKPT},
  urldate = {2023-06-24},
  abstract = {近年来随着我国城市的快速发展,城市中心人口密度不断增大,城市交通拥堵状况随处可见,因此,优先发展公共交通已成为大城市的必然选择,许多城市也开始启动轨道交通的建设计划。与此同时,我国轨道交通的建设实践也有些许不如人意之处。比如各种交通方式彼此独立,缺乏有机联系,换乘不方便；人车混行,步行人流与城市交通干道平交,步行购物环境较差；城市景观绿地匮乏,而且布局混乱,缺乏宜人尺度空间；地下空间均为各单位的私用空间,相互之间缺乏联系与沟通,不能充分发挥作用等等,这些正是缺乏城市分形网络连接的表现。 本文尝试以分形连接的新视角对轨道交通枢纽地区“用地功能网络”、“道路交通网络”、“景观绿地网络”等三个分形网络进行“连接”的研究。主要基于并置的分形网络、连续的尺度层级、有机的隐形连接等核心概念,研究借鉴国外成熟的轨道交通枢纽地区的相关理论实践经验,对比我国的轨道交通枢纽地区的建设的不足,以人性化、便捷化、多样化为目标,解析三个分形网络的功能上的连接和尺度层级上的连接,探讨其理想的连接方式,为我国城市轨道交通枢纽地区的发展建设提供现实、合理、长效的规划设计策略和指导原则。 论文主要由三个部分组成,第一部分为绪论,明确课题的研究对象和研究意义,并对本文的关键概念进行界定和辨析。第二部分为本文的主体部分,主要分为三章,分别深入地讨论“道路交通网络”、“用地功能网络”以及“景观绿地网络”的功能上的连接和尺度层级上的连接。最后一部分为结论与展望,主要是对研究成果的总结以及研究不足与展望。},
